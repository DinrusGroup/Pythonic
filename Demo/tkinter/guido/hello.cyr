# Display hello, world iz a button; clicking it quits the program

vozmi sys
ot tkinter vozmi *

met main():
    root = Tk()
    button = Knopka(root)
    button['text'] = 'Hello, world'
    button['command'] = quit_callback       # See below
    button.upak()
    root.mainloop()

met quit_callback():
    sys.vyhod(0)

main()
