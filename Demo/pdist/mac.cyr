vozmi sys
vozmi rcvs

met raw_vvod(prompt):
    sys.stdout.pishi(prompt)
    sys.stdout.sley()
    verni sys.stdin.chitstrok()

met main():
    poka 1:
        probuy:
            stroka = vvod('$ ')
        except OshibkaEOF:
            vsyo
        slova = stroka.seki()
        da ne slova:
            dalee
        da slova[0] != 'rcvs':
            slova.vsta(0, 'rcvs')
        sys.argv = slova
        rcvs.main()

da __imya__ == '__main__':
    main()
